<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eee"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/title"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:background="@drawable/title_bar"
        android:gravity="center_vertical" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="Search Result"
            android:textColor="#ffffff"
            android:textSize="20sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/result_frame1"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/eventlist_background2"
        android:clickable="true"
        android:gravity="center_vertical"
         >

        <ImageView
            android:id="@+id/result_head1"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:paddingLeft="5dp"
            android:src="@drawable/locator" />

        <TextView
            android:id="@+id/result_region1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@id/result_head1"
            android:padding="7dp"
            android:text=" Manhattan"
            android:textColor="#fff" />

        <TextView
            android:id="@+id/result_name1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/result_region1"
            android:layout_alignParentTop="true"
            android:padding="7dp"
            android:text="The New Standardsâ€™ Holiday Show "
            android:textColor="#fff" />

        <TextView
            android:id="@+id/result_hour1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="18dp"
            android:layout_toRightOf="@+id/result_region1"
            android:padding="7dp"
            android:text=" 18"
            android:textColor="#ff1" />

        <TextView
            android:id="@+id/result_TextView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/result_hour1"
            android:padding="7dp"
            android:text=":"
            android:textColor="#ff1" />

        <TextView
            android:id="@+id/result_m1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_toRightOf="@+id/result_minute1"
            android:padding="7dp"
            android:text="Dec18"
            android:textColor="#ff1" />

        <TextView
            android:id="@+id/result_minute1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/result_TextView01"
            android:padding="7dp"
            android:text=" 30"
            android:textColor="#ff1" />

    </RelativeLayout>
    


</LinearLayout>