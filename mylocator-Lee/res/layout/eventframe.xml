<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/eventframe"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/event_frame1"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/eventlist_background2"
        android:clickable="true"
        android:gravity="center_vertical"
        android:onClick="event1" >

        <ImageView
            android:id="@+id/head"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:paddingLeft="5dp"
            android:src="@drawable/locator" />

        <TextView
            android:id="@+id/event_region1"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@id/head"
            android:padding="7dp"
            android:text="Brooklyn"
            android:textColor="#fff" />

        <TextView
            android:id="@+id/event_m1"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:padding="7dp"
            android:text="pm"
            android:textColor="#ff1" />

        <TextView
            android:id="@+id/event_name1"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:layout_alignLeft="@+id/event_region1"
            android:layout_alignParentTop="true"
            android:padding="7dp"
            android:text="Maroon 5 Live"
            android:textColor="#fff" />

        <TextView
            android:id="@+id/event_hour1"
            android:layout_width="35dp"
            android:layout_height="30dp"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/TextView01"
            android:padding="7dp"
            android:text=" 01"
            android:textColor="#ff1" />

        <TextView
            android:id="@+id/event_minute1"
            android:layout_width="35dp"
            android:layout_height="30dp"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/event_m1"
            android:padding="7dp"
            android:text=" 01"
            android:textColor="#ff1" />

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="20dp"
            android:layout_height="30dp"
            android:layout_alignRight="@+id/event_name1"
            android:layout_below="@+id/event_name1"
            android:padding="7dp"
            android:text=":"
            android:textColor="#ff1" />

    </RelativeLayout>

</RelativeLayout>
